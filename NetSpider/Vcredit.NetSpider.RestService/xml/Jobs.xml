    <Job name="上海社保">
      <Steps>
        <Step sort="1" url="http://www.12333sh.gov.cn/grxx/login.jsw" methed="post" PostData="cxtype=1&amp;idcard=@{username}&amp;password=@{password}">
          <GatherDatas>
            <GatherData name="social" type="new" dataType="datatable">
              <GatherDataColumns>
                <GatherDataColumn colName="年月" start="&quot;>&lt;td&gt;" end="&lt;/td&gt;" limitSign="2001"/>
                <GatherDataColumn colName="缴费基数" start="&lt;td&gt;" end="&lt;/td&gt;" limitSign="2001"/>
