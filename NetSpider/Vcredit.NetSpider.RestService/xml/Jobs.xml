    <Job name="上海公积金">
      <Steps>
        <Step sort="1" url="https://persons.shgjj.com/VerifyImageServlet" methed="get"  resultType="byte">
          <GatherDatas>
            <GatherData name="imagecode" type="new">
            </GatherData>
          </GatherDatas>
        </Step>
        <Step sort="2" url="https://persons.shgjj.com/SsoLogin" methed="post" PostData="username=@{username}&amp;password=@{password}&amp;imagecode=@{imagecode}&amp;password_md5=@{password_MD5}&amp;ID=0">
          <Parameters>
            <Parameter name="imagecode"></Parameter>
            <Parameter name="username" type="input"></Parameter>
            <Parameter name="password" type="input"></Parameter>
            <Parameter name="password_MD5" type="input"></Parameter>
          </Parameters>
        </Step>
        <Step sort="3" url="http://bbs.shgjj.com/sso/sso.php?url=https://persons.shgjj.com/MainServlet?ID=1" methed="post" PostData="para1=@{username}&amp;para2=@{password_MD5}">
          <GatherDatas>
            <GatherData name="src" type="new">
              <Selectors>
                <Selector selectRegulation="//script[1]" selectAttribute="src"></Selector>
              </Selectors>
            </GatherData>
          </GatherDatas>
        </Step>
        <Step sort="4" url="@{src}" methed="get">
          <Parameters>
            <Parameter name="src"></Parameter>
          </Parameters>
        </Step>
        <Step sort="5" url="https://persons.shgjj.com/MainServlet?ID=1" methed="get">
          <GatherDatas>
            <GatherData name="name" output="1">
              <Selectors>
                <Selector start="width=&quot;751&quot;&gt;" end="&lt;strong&gt;" type="string"></Selector>
              </Selectors>
            </GatherData>
            <GatherData name="khdate" output="1">
              <Selectors>
                <Selector start="开户日期&lt;/div&gt;&lt;/td&gt;&lt;td&gt;" end="&lt;/td&gt;" type="string"></Selector>
              </Selectors>
            </GatherData>
            <GatherData name="account" output="1">
              <Selectors>
                <Selector start="公积金账号&lt;/div&gt;&lt;/td&gt;&lt;td&gt;" end="&lt;/td&gt;" type="string"></Selector>
              </Selectors>
            </GatherData>
            <GatherData name="company" output="1">
              <Selectors>
                <Selector start="所属单位&lt;/div&gt;&lt;/td&gt;&lt;td&gt;" end="&lt;/td&gt;" type="string"></Selector>
              </Selectors>
            </GatherData>
            <GatherData name="Amount" output="1">
              <Selectors>
                <Selector start="账户余额&lt;/div&gt;&lt;/td&gt;&lt;td&gt;" end="&lt;/td&gt;" type="string"></Selector>
              </Selectors>
            </GatherData>
          </GatherDatas>
        </Step>
        <Step sort="6" url="https://persons.shgjj.com/MainServlet?ID=11" methed="get">
          <GatherDatas>
            <GatherData name="detail" type="new" dataType="datatable">
              <GatherDataColumns>
                <GatherDataColumn colName="date" start="&lt;td&gt;&lt;div align=&quot;center&quot;&gt;" end="&lt;/div&gt;&lt;/td&gt;" limitSign="2001"/>
                <GatherDataColumn colName="company" start="style=&quot; padding:2px;&quot;&gt;" end="&lt;/div&gt;&lt;/td&gt;" limitSign="2001"/>
                <GatherDataColumn colName="amount" start="&lt;td&gt;&lt;div align=&quot;right&quot; style=&quot; padding:2px;&quot;&gt;" end="&lt;/div&gt;&lt;/td&gt;" limitSign="2001"/>
                <GatherDataColumn colName="desc" start="&lt;td&gt;&lt;div align=&quot;center&quot;&gt;" end="&lt;/div&gt;&lt;/td&gt;" limitSign="2001"/>
                <GatherDataColumn colName="note" start="&lt;td&gt;&lt;div align=&quot;center&quot;&gt;" end="&lt;/div&gt;&lt;/td&gt;" limitSign="2001"/>
              </GatherDataColumns>
              <Selectors>
                <Selector selectRegulation="//table[@class='table']/tr[position() >2]" selectAttribute="" type="html"></Selector>
              </Selectors>
            </GatherData>
          </GatherDatas>
        </Step>
      </Steps>
    </Job>
