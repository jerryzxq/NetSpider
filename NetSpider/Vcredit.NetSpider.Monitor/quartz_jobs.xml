<?xml version="1.0" encoding="UTF-8"?>

<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 				version="2.0">

  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>

  <schedule>
    <!--mobileWebExcJob:网站不稳定；mobileProExcJob:程序异常；mobileRevisionExcJob:网站改版；mobileWebUpdateJob:网站升级；mobileAnalysisSucceedRatioJob:网站解析成功比；mobileColmonJob:必要信息监控-->
    <job>
      <name>mobileWebExcJob</name>
      <group>mobileWebExcGroup</group>
      <description>jobDesciption1</description>
      <job-type>Vcredit.NetSpider.Monitor.job.MobileWebExcJob, Vcredit.NetSpider.Monitor</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>

    <job>
      <name>mobileProExcJob</name>
      <group>mobileProExcGroup</group>
      <description>jobDesciption1</description>
      <job-type>Vcredit.NetSpider.Monitor.job.MobileProExcJob, Vcredit.NetSpider.Monitor</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>

    <job>
      <name>mobileRevisionExcJob</name>
      <group>mobileRevisionExcGroup</group>
      <description>jobDesciption1</description>
      <job-type>Vcredit.NetSpider.Monitor.job.MobileRevisionExcJob, Vcredit.NetSpider.Monitor</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>

    <job>
      <name>mobileWebUpdateJob</name>
      <group>mobileWebUpdateGroup</group>
      <description>jobDesciption1</description>
      <job-type>Vcredit.NetSpider.Monitor.job.MobileWebUpdateJob, Vcredit.NetSpider.Monitor</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>

    <job>
      <name>mobileAnalysisSucceedRatioJob</name>
      <group>mobileAnalysisSucceedRatioGroup</group>
      <description>jobDesciption1</description>
      <job-type>Vcredit.NetSpider.Monitor.job.MobileAnalysisSucceedRatioJob, Vcredit.NetSpider.Monitor</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>

    <job>
      <name>mobileColmonJob</name>
      <group>mobileColmonGroup</group>
      <description>jobDesciption1</description>
      <job-type>Vcredit.NetSpider.Monitor.job.MobileColmonJob, Vcredit.NetSpider.Monitor</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    
    
    <trigger>
      <cron>
        <name>mobileWebExcCronName</name>
        <group>mobileWebExcCronGroup</group>
        <description></description>
        <job-name>mobileWebExcJob</job-name>
        <job-group>mobileWebExcGroup</job-group>
        <cron-expression> 0 0 0/1 * * ?</cron-expression>
      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>mobileProExcCronName</name>
        <group>mobileProExcCronGroup</group>
        <description></description>
        <job-name>mobileProExcJob</job-name>
        <job-group>mobileProExcGroup</job-group>
        <cron-expression> 0 0 0/1 * * ?</cron-expression>
      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>mobileRevisionExcCronName</name>
        <group>mobileRevisionExcCronGroup</group>
        <description></description>
        <job-name>mobileRevisionExcJob</job-name>
        <job-group>mobileRevisionExcGroup</job-group>
        <cron-expression> 0 0 0/1 * * ?</cron-expression>
      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>mobileWebUpdateCronName</name>
        <group>mobileWebUpdateCronGroup</group>
        <description></description>
        <job-name>mobileWebUpdateJob</job-name>
        <job-group>mobileWebUpdateGroup</job-group>
        <cron-expression> 0 0 0/1 * * ?</cron-expression>
      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>mobileAnalysisSucceedRatioCronName</name>
        <group>mobileAnalysisSucceedRatioCronGroup</group>
        <description></description>
        <job-name>mobileAnalysisSucceedRatioJob</job-name>
        <job-group>mobileAnalysisSucceedRatioGroup</job-group>
        <cron-expression> 0 0 0/1 * * ?</cron-expression>
      </cron>
    </trigger>

    <trigger>
      <cron>
        <name>mobileColmonCronName</name>
        <group>mobileColmonCronGroup</group>
        <description></description>
        <job-name>mobileColmonJob</job-name>
        <job-group>mobileColmonGroup</job-group>
        <cron-expression> 0 0 0/1 * * ?</cron-expression>
      </cron>
    </trigger>
    
  </schedule>
  
</job-scheduling-data>
