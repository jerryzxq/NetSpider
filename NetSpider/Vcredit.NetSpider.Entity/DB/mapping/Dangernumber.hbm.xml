    <property name="Phone" type="String">
      <column name="Phone" length="30" sql-type="varchar" not-null="false"/>
    </property>
    <property name="City" type="String">
      <column name="City" length="50" sql-type="varchar" not-null="false"/>
    </property>
    <property name="IsOwn" type="Boolean">
      <column name="IsOwn" length="1" sql-type="bit" not-null="false"/>
    </property>
    <property name="Description" type="String">
      <column name="Description" length="500" sql-type="varchar" not-null="false"/>
    </property>
    <property name="IPAddr" type="String">
