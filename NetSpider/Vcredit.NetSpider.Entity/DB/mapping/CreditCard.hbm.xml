<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Vcredit.NetSpider.Entity.DB.CreditCardEntity, Vcredit.NetSpider.Entity"   table="CreditCard">
		<id name="LoancardId" type="Int32" unsaved-value="null">
			<column name="LoancardId" length="4" sql-type="int" not-null="true" unique="true" index="PK_CreditCard"/>
			<generator class="native" />
		</id>
		<property name="ReportId" type="StringClob">
			<column name="ReportId" length="10" sql-type="nchar" not-null="false"/>
		</property>
		<property name="Cue" type="String">
			<column name="Cue" length="500" sql-type="varchar" not-null="false"/>
		</property>
		<property name="HighestAmount" type="Decimal">
			<column name="Highest_Amount" length="9" sql-type="decimal" not-null="false"/>
		</property>
		<property name="UsedAmount" type="Decimal">
			<column name="Used_Amount" length="9" sql-type="decimal" not-null="false"/>
		</property>
		<property name="OpenTime" type="DateTime">
			<column name="OpenTime" length="4" sql-type="smalldatetime" not-null="false"/>
		</property>
	</class>
</hibernate-mapping>
