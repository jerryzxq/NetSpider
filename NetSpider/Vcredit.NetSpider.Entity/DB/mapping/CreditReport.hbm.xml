<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Vcredit.NetSpider.Entity.DB.CreditReportEntity, Vcredit.NetSpider.Entity"   table="CreditReport">
		<id name="ReportId" type="String" unsaved-value="null">
			<column name="ReportId" length="100" sql-type="varchar" not-null="true" unique="true" index="PK_CreditReport"/>
			<generator class="assigned" />
		</id>
		<property name="ParentId" type="Int32">
			<column name="ParentId" length="4" sql-type="int" not-null="false"/>
		</property>
		<property name="ReportTime" type="DateTime">
			<column name="ReportTime" length="4" sql-type="smalldatetime" not-null="false"/>
		</property>
		<property name="QueryTime" type="DateTime">
			<column name="QueryTime" length="4" sql-type="smalldatetime" not-null="false"/>
		</property>
		<property name="Name" type="String">
			<column name="Name" length="100" sql-type="varchar" not-null="false"/>
		</property>
		<property name="Gender" type="String">
			<column name="Gender" length="2" sql-type="varchar" not-null="false"/>
		</property>
		<property name="CertType" type="String">
			<column name="CertType" length="30" sql-type="varchar" not-null="false"/>
		</property>
		<property name="CertNo" type="String">
			<column name="CertNo" length="20" sql-type="varchar" not-null="false"/>
		</property>
		<property name="MaritalState" type="String">
			<column name="MaritalState" length="10" sql-type="varchar" not-null="false"/>
		</property>
		<property name="CreateTime" type="DateTime" insert="false">
			<column name="CreateTime" length="4" sql-type="smalldatetime" not-null="false"/>
		</property>
	</class>
</hibernate-mapping>
