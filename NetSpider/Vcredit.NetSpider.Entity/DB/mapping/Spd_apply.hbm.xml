<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Vcredit.NetSpider.Entity.DB.Spd_applyEntity, Vcredit.NetSpider.Entity"   table="spd_apply">
    <id name="ApplyId" type="Int32" unsaved-value="null">
      <column name="applyId" length="4" sql-type="int" not-null="true" unique="true" index="PK_spd_apply"/>
      <generator class="native" />
    </id>
    <property name="Identitycard" type="String">
      <column name="identitycard" length="50" sql-type="varchar" not-null="false"/>
    </property>
    <property name="Name" type="String">
      <column name="name" length="50" sql-type="varchar" not-null="false"/>
    </property>
    <property name="Mobile" type="String">
      <column name="mobile" length="30" sql-type="varchar" not-null="false"/>
    </property>
    <property name="Website" type="String">
      <column name="website" length="50" sql-type="varchar" not-null="false"/>
    </property>
    <property name="Spider_type" type="String">
      <column name="spider_type" length="50" sql-type="varchar" not-null="false"/>
    </property>
    <property name="AppId" type="String">
      <column name="appId" length="50" sql-type="varchar" not-null="false"/>
    </property>
    <property name="Token" type="String">
      <column name="token" length="100" sql-type="varchar" not-null="false"/>
    </property>
    <property name="CreateTime" type="DateTime">
      <column name="createtime" length="4" sql-type="smalldatetime" not-null="false"/>
    </property>
    <property name="Apply_status" type="Int32">
      <column name="apply_status" length="4" sql-type="int" not-null="false"/>
    </property>
    <property name="Crawl_status" type="Int32">
      <column name="crawl_status" length="4" sql-type="int" not-null="false"/>
    </property>
    <property name="Description" type="String">
      <column name="description" length="300" sql-type="varchar" not-null="false"/>
    </property>
    <property name="IPAddr" type="String">
      <column name="IPAddr" length="60" sql-type="varchar" not-null="false"/>
    </property>
  </class>
</hibernate-mapping>