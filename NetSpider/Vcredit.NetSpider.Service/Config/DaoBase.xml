  <!-- 事务管理策略，本地数据库事务 -->
  <object id="Vcredit.NetSpider.transactionManager"
       type="Spring.Data.NHibernate.HibernateTransactionManager, Spring.Data.NHibernate4">
    <property name="DbProvider" ref="Vcredit.NetSpider.DbProvider"/>
    <property name="SessionFactory" ref="Vcredit.NetSpider.NHibernateSessionFactory"/>
  </object>


  <object id="Vcredit.NetSpider.transactionInterceptor" type="Spring.Transaction.Interceptor.TransactionInterceptor, Spring.Data">
    <property name="TransactionManager" ref="Vcredit.NetSpider.transactionManager"/>
    <property name="TransactionAttributeSource">
      <object type="Spring.Transaction.Interceptor.AttributesTransactionAttributeSource, Spring.Data"/>
    </property>
  </object>

  <object id="Vcredit.NetSpider.BaseTransactionManager"  type="Spring.Transaction.Interceptor.TransactionProxyFactoryObject, Spring.Data" abstract="true">
    <property name="PlatformTransactionManager" ref="Vcredit.NetSpider.transactionManager"/>
    <property name="TransactionAttributes">
      <name-values>
        <add key="Save*" value="PROPAGATION_REQUIRED"/>
        <add key="Update*" value="PROPAGATION_REQUIRED"/>
        <add key="Delete*" value="PROPAGATION_REQUIRED"/>
      </name-values>
    </property>
  </object>

  <!--将id为NHibernateSessionFactory的对象注入到HibernateTemplate中-->
  <object id="Vcredit.NetSpider.HibernateTemplate" type="Spring.Data.NHibernate.Generic.HibernateTemplate">
    <property name="SessionFactory" ref="Vcredit.NetSpider.NHibernateSessionFactory" />
    <property name="TemplateFlushMode" value="Auto" />
    <property name="CacheQueries" value="true" />
  </object>

  <!--日志接口配置文件-->
  <object id="Vcredit.NetSpider.LogService" type="Vcredit.Framework.Common.Utility.Log4netAdapter,Vcredit.Framework.Common.Utility" >
  </object>


